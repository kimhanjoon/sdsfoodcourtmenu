<!-- Nav tabs -->
<ul class="nav nav-tabs" role="tablist">
  <li role="presentation" class="text-center {{#is time 'breakfast'}}active{{/is}}" style="width:33%;">
    <a href="#breakfast-tab" aria-controls="breakfast" role="tab" data-toggle="tab">
      <span class="kor">아침</span><span class="eng hidden">Breakfast</span>
    </a>
  </li>
  <li role="presentation" class="text-center {{#is time 'lunch'}}active{{/is}}" style="width:34%;">
    <a href="#lunch-tab" aria-controls="lunch" role="tab" data-toggle="tab">
      <span class="kor">점심</span><span class="eng hidden">Lunch</span>
    </a>
  </li>
  <li role="presentation" class="text-center {{#is time 'dinner'}}active{{/is}}" style="width:33%;">
    <a href="#dinner-tab" aria-controls="dinner" role="tab" data-toggle="tab">
      <span class="kor">저녁</span><span class="eng hidden">Dinner</span>
    </a>
  </li>
</ul>

<!-- Tab panes -->
<div class="tab-content">

  <div role="tabpanel" class="tab-pane fade {{#is time 'breakfast'}}in active{{/is}}" id="breakfast-tab">
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층</span><span class="eng hidden">B1</span></div>
      {{> jamsilMenu foods.breakfast.b1}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층 Snap snack</span><span class="eng hidden">B1 Snap snack</span></div>
      {{> jamsilMenu foods.breakfast.b1s}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층 Take-out</span><span class="eng hidden">B1 Take-out</span></div>
      {{> jamsilMenu foods.breakfast.b1t}}
    </div>
  </div>

  <div role="tabpanel" class="tab-pane fade {{#is time 'lunch'}}in active{{/is}}" id="lunch-tab">
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층</span><span class="eng hidden">B1</span></div>
      {{> jamsilMenu foods.lunch.b1}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 2층</span><span class="eng hidden">B2</span></div>
      {{> jamsilMenu foods.lunch.b2}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층 Snap snack</span><span class="eng hidden">B1 Snap snack</span></div>
      {{> jamsilMenu foods.lunch.b1s}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층 Take-out</span><span class="eng hidden">B1 Take-out</span></div>
      {{> jamsilMenu foods.lunch.b1t}}
    </div>
  </div>

  <div role="tabpanel" class="tab-pane fade {{#is time 'dinner'}}in active{{/is}}" id="dinner-tab">
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층</span><span class="eng hidden">B1</span></div>
      {{> jamsilMenu foods.dinner.b1}}
    </div>
    <div class="panel panel-primary">
      <div class="panel-heading"><span class="kor">지하 1층 Snap snack</span><span class="eng hidden">B1 Snap snack</span></div>
      {{> jamsilMenu foods.dinner.b1s}}
    </div>
  </div>

</div>


<div class="panel panel-default">
	<div class="panel-heading"><span class="kor">운영시간 안내</span><span class="eng hidden">Hours of Operation</span></div>
    <ul class='list-group'>
        <li class="list-group-item">
            <strong><span class="kor">월요일 - 금요일</span><span class="eng hidden">Monday - Friday</span></strong><br>
            <span class="kor">아침</span><span class="eng hidden">Breakfast</span> 06:30 - 08:15<br>
            <span class="kor">점심</span><span class="eng hidden">Lunch</span> 11:30 - 13:15<br>
            <span class="kor">저녁</span><span class="eng hidden">Dinner</span> 18:00 - 19:00
        </li>
        <li class="list-group-item">
    		<strong><span class="kor">토요일</span><span class="eng hidden">Saturday</span></strong><br>
    		<span class="kor">점심</span><span class="eng hidden">Lunch</span> 11:30 - 12:30
        </li>
    </ul>
</div>

